<odoo>
    <data>
        <record id="test_form" model="ir.ui.view">
            <field name="name">test</field>
            <field name="model">test.test</field>
            <field name="arch" type="xml">
                <form string="form">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

    </data>


    <record id="test_tree" model="ir.ui.view">
        <field name="name">test</field>
        <field name="model">test.test</field>
        <field name="arch" type="xml">
            <tree string="tree">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="test_action" model="ir.actions.act_window">
        <field name="name"></field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">test.test</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
          <p class="oe_view_nocontent_create">
            <!-- Add Text Here -->
          </p><p>
            <!-- More details about what a user can do with this object will be OK -->
          </p>
        </field>
    </record>

    <!-- This Menu Item must have a parent and an action -->
    <menuitem id="UNIQUE_ID_categ" name="test"  action="test_action" />
</odoo>
